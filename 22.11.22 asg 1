sno=int(input('enter the student roll no:'))
name=input('enter the student name:')
gender=input('enter student gender:')
address=input('enter the student address')
phno=int(input('enter the student phone number:'))
print('student rollno','is',sno)
print('student name','is',name)
print('student gender','is',gender)
print('student address','is',address)
print('student phno','is',phno)
